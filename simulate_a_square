class square:
  def__init__(self,height="0",width="0"):
    self.height = height
    self.width = width
  @property
  def height(self):
    print("retriving height")
    return self.__height
  @height.setter
  def height(self.value):
    if value.isdigit():
      self.__height = value
    else:
      print("please only enter number for height:")
def main():
  @property
  def width(self):
    print("retrieving width")
